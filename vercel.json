{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/auth/(.*)",
      "destination": "/"
    },
    {
      "source": "/admin/(.*)",
      "destination": "/"
    },
    {
      "source": "/shop/(.*)",
      "destination": "/"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
